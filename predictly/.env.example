# Movement Network Configuration
# Copy this file to .env and fill in your values

# Network (devnet, testnet, mainnet)
MOVEMENT_NETWORK=devnet

# Your deployer account private key (hex format without 0x prefix)
# Generate with: movement init --network devnet
MOVEMENT_PRIVATE_KEY=your_private_key_here

# Your deployer account address
MOVEMENT_ADDRESS=your_address_here

# RPC URL (optional, defaults to devnet)
MOVEMENT_RPC_URL=https://devnet.m1.movementlabs.xyz
